{
  "name": "onboard",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer",
      "@emotion/jest/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/**/*.{test,spec,stories}.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
